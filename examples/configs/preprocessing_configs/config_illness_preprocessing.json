{
    "prefill_training_configs": true,
    "use_label_col_as_discrete_metadata": false,
    "add_lag_data": false,
    "window_from_beginning": false,
    "shuffle": false,
    "train_val_split": {
        "train": 0.8,
        "val": 0.1
    },
    "filename": "illness/illness_daily_ts_10k.parquet",
    "window_size": 264,
    "stride": 32,
    "timestamps_col": ["day"],
    "history_forecast_split": {
        "history_length": 256
    },
    "group_labels": {
        "cols": [
            "user_id"
        ],
        "scalers": {}
    },
    "timeseries": {
        "cols": [
            "tag_any_sick"
        ],
        "scalers": {}
    },
    "discrete": {
        "cols": [
        ],
        "scalers": {}
    },
    "continuous": {
        "cols": [
            "average_hrv",
            "lowest_heart_rate",
            "age_cva_diff",
            "highest_temperature",
            "stressed_duration",
            "latency",
            "bmi",
            "hrv_std",
            "awake_mins",
            "age",
            "readiness_score",
            "sleep_score",
            "low_activity_time",
            "deep_mins",
            "sleep_duration",
            "restored_duration",
            "non_wear_time",
            "rem_mins",
            "steps",
            "active_calories",
            "heart_rate_std",
            "sedentary_time",
            "hours_after_oura_sleep_start",
            "light_mins"
        ],
        "scalers": {}
    }
}