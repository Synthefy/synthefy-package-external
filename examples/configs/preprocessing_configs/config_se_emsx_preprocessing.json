{
    "prefill_training_configs": true,
    "use_label_col_as_discrete_metadata": true,
    "window_from_beginning": false,
    "shuffle": false,
    "train_val_split": {
        "train": 0.8,
        "val": 0.1
    },
    "filename": "se_emsx/EMSx.parquet",
    "window_size": 192,
    "stride": 24,
    "timestamps_col": [
        "timestamp"
    ],
    "timestamps": {
        "timestamps_features": [
            "M",
            "D",
            "H",
            "T"
        ]
    },
    "group_labels": {
        "cols": [
            "site_id"
        ],
        "scalers": {}
    },
    "timeseries": {
        "cols": [
            "actual_consumption",
            "actual_pv"
        ],
        "scalers": {}
    },
    "discrete": {
        "cols": [],
        "scalers": {}
    },
    "continuous": {
        "cols": [
            "max_load",
            "capacity",
            "power",
            "charge_efficiency",
            "discharge_efficiency"
        ],
        "scalers": {
            "max_load": {
                "scaler_type": "minmax"
            },
            "capacity": {
                "scaler_type": "minmax"
            },
            "power": {
                "scaler_type": "minmax"
            },
            "charge_efficiency": {
                "scaler_type": "minmax"
            },
            "discharge_efficiency": {
                "scaler_type": "minmax"
            }
        }
    }
}